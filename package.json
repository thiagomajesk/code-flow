{
  "name": "code-flow",
  "displayName": "Code Flow",
  "description": "Visual Studio Code Extension For Productivity Junkies",
  "preview": true,
  "license": "AGPL-3.0",
  "version": "0.0.3",
  "icon": "static/code-flow-logo.png",
  "publisher": "tmg",
  "homepage": "https://github.com/thiagomajesk/code-flow",
  "repository": {
    "type": "git",
    "url": "https://github.com/thiagomajesk/code-flow"
  },
  "engines": {
    "vscode": "^1.55.0"
  },
  "categories": [
    "Keymaps",
    "Extension Packs",
    "Other"
  ],
  "keywords": ["productivity", "vim-like", "emacs-like", "shortcuts", "awesome"],
  "activationEvents": ["onStartupFinished"],
  "main": "./out/src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "code-flow.smartTravelUp",
        "title": "Code Flow: Smart Travel Up"
      },
      {
        "command": "code-flow.smartTravelDown",
        "title": "Code Flow: Smart Travel Down"
      },
      {
        "command": "code-flow.smartSelectUp",
        "title": "Code Flow: Smart Select Up"
      },
      {
        "command": "code-flow.smartSelectDown",
        "title": "Code Flow: Smart Select Down"
      }
    ],
    "keybindings": [
      {
        "key": "ctrl+k ctrl+e",
        "command": "workbench.view.explorer"
      },
      {
        "key": "ctrl+k ctrl+f",
        "command": "workbench.view.search"
      },
      {
        "key": "ctrl+k ctrl+g",
        "command": "workbench.view.scm"
      },
      {
        "key": "ctrl+k ctrl+d",
        "command": "workbench.view.debug"
      },
      {
        "key": "ctrl+k ctrl+x",
        "command": "workbench.extensions.action.showInstalledExtensions"
      },
      {
        "key": "ctrl+k ctrl+s",
        "command": "workbench.action.toggleSidebarVisibility"
      },
      {
        "key": "ctrl+k ctrl+r",
        "command": "targetsWsl.focus"
      },
      {
        "key": "ctrl+k ctrl+b",
        "command": "workbench.action.focusActiveEditorGroup"
      },
      {
        "key": "ctrl+k ctrl+t",
        "command": "workbench.action.terminal.toggleTerminal"
      },
      {
        "key": "ctrl+k ctrl+m",
        "command": "editor.action.toggleMinimap"
      },
      {
        "key": "ctrl+d",
        "command": "fileutils.duplicateFile",
        "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsRoot && !inputFocus"
      },
      {
        "key": "ctrl+n",
        "command": "explorer.newFile",
        "when": "explorerViewletVisible && filesExplorerFocus && !inputFocus"
      },
      {
        "key": "ctrl+shift+n",
        "command": "explorer.newFolder",
        "when": "explorerViewletVisible && filesExplorerFocus && !inputFocus"
      },
      {
        "key": "ctrl+alt+n",
        "command": "fileutils.newFileAtRoot",
        "when": "explorerViewletVisible && filesExplorerFocus && !inputFocus"
      },
      {
        "key": "ctrl+shift+alt+n",
        "command": "fileutils.newFolderAtRoot",
        "when": "explorerViewletVisible && filesExplorerFocus && !inputFocus"
      },
      {
        "key": "ctrl+b ctrl+w",
        "command": "workbench.action.joinAllGroups",
        "when": "editorFocus"
      },
      {
        "key": "ctrl+b ctrl+n",
        "command": "workbench.action.splitEditor",
        "when": "editorFocus"
      },
      {
        "key": "ctrl+b ctrl+p",
        "command": "markdown.showPreviewToSide",
        "when": "editorFocus"
      },
      {
        "key": "ctrl+b ctrl+r",
        "command": "workbench.action.reopenClosedEditor",
        "when": "editorFocus"
      },
      {
        "key": "ctrl+b ctrl+oem_plus",
        "command": "workbench.action.splitEditorOrthogonal",
        "when": "editorFocus"
      },
      {
        "key": "ctrl+b ctrl+oem_minus",
        "command": "workbench.action.splitEditorDown",
        "when": "editorFocus"
      },
      {
        "key": "ctrl+b right",
        "command": "workbench.action.navigateRight",
        "when": "editorFocus"
      },
      {
        "key": "ctrl+b ctrl+right",
        "command": "workbench.action.moveEditorToRightGroup",
        "when": "editorFocus"
      },
      {
        "key": "ctrl+b down",
        "command": "workbench.action.navigateDown",
        "when": "editorFocus"
      },
      {
        "key": "ctrl+b ctrl+down",
        "command": "workbench.action.moveEditorToBellowGroup",
        "when": "editorFocus"
      },
      {
        "key": "ctrl+b left",
        "command": "workbench.action.navigateLeft",
        "when": "editorFocus"
      },
      {
        "key": "ctrl+b ctrl+left",
        "command": "workbench.action.moveEditorToLeftGroup",
        "when": "editorFocus"
      },
      {
        "key": "ctrl+b up",
        "command": "workbench.action.navigateUp",
        "when": "editorFocus"
      },
      {
        "key": "ctrl+b ctrl+up",
        "command": "workbench.action.moveEditorToAboveGroup",
        "when": "editorFocus"
      },
      {
        "key": "ctrl+b ctrl+a",
        "command": "workbench.action.showAllEditors"
      },
      {
        "key": "ctrl+b ctrl+g",
        "command": "workbench.action.showEditorsInActiveGroup"
      },
      {
        "key": "ctrl+b ctrl+n",
        "command": "workbench.action.terminal.split",
        "when": "terminalFocus"
      },
      {
        "key": "ctrl+b right",
        "command": "workbench.action.terminal.focusNextPane",
        "when": "terminalFocus"
      },
      {
        "key": "ctrl+b left",
        "command": "workbench.action.terminal.focusPreviousPane",
        "when": "terminalFocus"
      },
      {
        "key": "ctrl+b ctrl+w",
        "command": "workbench.action.terminal.kill",
        "when": "terminalFocus"
      },
      {
        "key": "ctrl+shift+oem_6",
        "command": "editor.fold",
        "when": "editorFocus"
      },
      {
        "key": "ctrl+shift+oem_5",
        "command": "editor.unfold",
        "when": "editorFocus"
      },
      {
        "key": "ctrl+oem_6",
        "command": "editor.action.indentLines",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "key": "ctrl+oem_5",
        "command": "editor.action.outdentLines",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "key": "ctrl+j",
        "command": "editor.action.joinLines",
        "when": "editorTextFocus"
      },
      {
        "key": "alt+t u",
        "command": "editor.action.transformToUppercase",
        "when": "editorTextFocus"
      },
      {
        "key": "alt+t l",
        "command": "editor.action.transformToLowercase",
        "when": "editorTextFocus"
      },
      {
        "key": "alt+t t",
        "command": "editor.action.transformToTitlecase",
        "when": "editorTextFocus"
      },
      {
        "key": "ctrl+shift+d",
        "command": "deleteInsideWord",
        "when": "editorTextFocus"
      },
      {
        "key": "alt+right",
        "command": "editor.action.moveCarretRightAction",
        "when": "editorTextFocus"
      },
      {
        "key": "alt+left",
        "command": "editor.action.moveCarretLeftAction",
        "when": "editorTextFocus"
      },
      {
        "command": "code-flow.smartTravelUp",
        "key": "ctrl+up",
        "when": "editorTextFocus"
      },
      {
        "command": "code-flow.smartTravelDown",
        "key": "ctrl+down",
        "when": "editorTextFocus"
      },
      {
        "command": "code-flow.smartSelectUp",
        "key": "ctrl+shift+up",
        "when": "editorTextFocus"
      },
      {
        "command": "code-flow.smartSelectDown",
        "key": "ctrl+shift+down",
        "when": "editorTextFocus"
      }
    ],
    "configuration": {
      "title": "Code Flow Configuration",
      "type": "object",
      "properties": {
        "code-flow.languageMarks": {
          "type": "object",
          "description": "Language Marks Definition",
          "default": {
            "typescript": [
              "^(switch|if|else|var|module|public|private|enum|export|for|while|return|function|try|catch|return|yield|const)"
            ]
          }
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.55.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "eslint": "^7.19.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}